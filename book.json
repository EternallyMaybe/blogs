{
    "title": "Blogs",
    "author": "Eternally <908624042@qq.com>",
    "description": "This is a sample book created by gitbook",
    "extension": null,
    "generator": "site",
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "links": {
        "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "search-pro",
        "expandable-chapters-small",
        "back-to-top-button",
        "code",
        "popup",
        "github",
        "tbfed-pagefooter",
        "splitter",
        "insert-logo",
        "page-toc-button"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/EternallyMaybe"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &#169; Eternally",
            "modify_label": "modified at",
            "modify_format": "YYYY-MM-DD hh:mm:ss"
        },
        "insert-logo": {
            "url": "/src/images/gitbook-logo.jpg",
            "style": "background: none; width: 100%;"
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
   		}
    }
}